    "husky": "0.14.3",
    "jest": "22.4.3",
    "prettier": "1.11.1",
    "pretty-quick": "1.4.1"
  },
  "jest": {
    "coverageDirectory": ".jest-coverage",
    "coverageThreshold": {
      "global": {
