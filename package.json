{
  "name": "chance-path",
  "description": "A Chance.js mixin to generate paths.",
  "version": "1.0.0",
  "author": {
    "name": "Michael Novotny",
    "email": "manovotny@gmail.com",
    "url": "htts://manovotny.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manovotny/chance-path.git"
  },
  "keywords": [
    "chance",
    "generated",
    "mixin",
    "path",
    "random"
  ],
